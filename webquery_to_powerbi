x = requests.get(url1)
html = etree.HTML(x.content)
tr_nodes = html.xpath('//table/tr')

td_content = [[td.text for td in tr.xpath('td')] for tr in tr_nodes[0:]]

data = pd.DataFrame(td_content)

header = data.iloc[0]
data = data[1:]
data.columns = header




